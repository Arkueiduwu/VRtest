[display]

window/size/viewport_width=1832
window/size/viewport_height=1920
window/snap/snap_2d_transforms_to_pixel=true

[dotnet]

project/assembly_name="newVR"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-xr-tools/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "green",
"res://scenes/": "red",
"res://script/": "blue"
}

[global_group]

grabbable=""
gun=""
player=""

[rendering]

2d/snap/snap_2d_transforms_to_pixel=true

[xr]

